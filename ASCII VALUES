'''
n=input()
for i in n:
   #if(ord(i)>=60 and ord(i)>=90): #ascii values for capital letters
    n=ord(i)+4
    print(chr(n),end="")
'''
#ip:hello-----wor----ld
#op:------helloworld
inp=input()
count=0
ans=""
for i in inp:
   if(i=="-"):
      count+=1
   else:
      ans=ans+i
print("-"*count+ans)


